<div class="container">
  <div class="row">
    <div class="col-md-4">
      <%= render 'users/menu' %>
    </div>
    <div class="col-md-8">
      <%= render 'users/info', user: @user %>
      <h2 class="font-weight-bold text-center"><i class="fas fa-edit"></i>自分の投稿</h2>
      <%= render 'post_images/each', post_images: @post_images %>
      <%= paginate(@post_images) %>
    </div>
  </div>
</div>
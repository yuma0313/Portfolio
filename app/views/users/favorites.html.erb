<div class="container px-5 px-sm-0">
    <div class="row">
        <div class="col-md-4">
            <%#= attachment_image_tag @user, :profile_image, :fill, 60, 60, fallback: "no_image.jpg"%>
          <table class="table">
            <tbody>
              <tr><td>ホーム</td></tr>
              <tr><td>投稿</td></tr>
              <tr><td><%= link_to "いいね", favorites_users_path %></td></tr>
              <tr><td>ブックマーク</td></tr>
              <tr><td>プロフィール</td></tr>
              <tr><td>イベント</td></tr>
            </tbody>
          </table>
        </div>
        <div class="col-md-7 offset-md-1">
          <h2>いいね一覧</h2>
          <table class="table table-hover table-inverse">
            <thead>
              <tr>
                <th></th>
                <th>カフェ名</th>
                <th>キャプション</th>
                <th></th>
                <th colspan="3"></th>
              </tr>
            </thead>
            <tbody>
             <% current_user.favorited_post_images.each do |post_image| %>
              <tr>
                <td></td>
                <td><%= link_to post_image.name,post_image_path(post_image.id) %></td>
                <td><%= post_image.caption %></td>
              </tr>
              <%= attachment_image_tag post_image, :image %>
             <% end %>
            </tbody>
          </table>
        </div>
    </div>
</div>